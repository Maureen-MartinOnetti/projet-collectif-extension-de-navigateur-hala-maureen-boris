{
    "manifest_version": 3,
    "name": "Speed Search",
    "version": "1.0",
    "description": "Extension for speed search",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  